<html>
  <h3>Current Pharmacy:</h3>
  <table>
    <thead>
      <th>Pharmacy ID</th>
      <th>Pharmacy Name</th>
      <th>Pharmacy Address</th>
      <th>Pharmacy Contact</th>
    </thead>
    <tbody>
      {{#each pharmacyhd}}
        <tr>
          <td>{{pharmacy_id}}</td>
          <td>{{pharmacy_name}}</td>
          <td>{{pharmacy_address}}</td>
          <td>{{pharmacy_contact}}</td>
        </tr>
      {{/each}}
    </tbody>
  </table>
</html>